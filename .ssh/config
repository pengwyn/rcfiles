	ServerAliveInterval 15
	ServerAliveCountMax 20
	TCPKeepAlive no
	ControlPath ~/.ssh/master-%r@%h:%p
	ControlMaster auto
	ControlPersist 15m
	Compression yes
	VisualHostKey yes
	SendEnv LANG

